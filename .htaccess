RewriteEngine on

Options -Indexes

RewriteCond %{HTTP_HOST} ^www\.rapidautoshipping\.com [NC]
RewriteRule ^(.*)$ https://rapidautoshipping.com/$1 [L,R=301]


# This rule alow user to access file
RewriteCond %{REQUEST_FILENAME} !-f
#=============================================================================================================
#
ErrorDocument 404 https://rapidautoshipping.com/404
#

#
#=============================================================================================================
#=============================================================================================================
#
# Home page route
ReWriteRule ^$ pages/home.php [NC,L]

#
#=============================================================================================================
#
# This route work for sub service page (car-transport-in-us)
ReWriteRule ^services/([0-9A-Za-z\-]+)([/]?)$ pages/services-pages/$1.php [NC,L]
ReWriteRule ^services([/]?)$ pages/service.php [NC,L]
#
# This route work for sub state-to state page (car-transport-in-us)
ReWriteRule ^state-to-state-transport/([0-9A-Za-z\-]+)([/]{0,1}+)$ pages/state-to-state-transport/$1.php [NC,L]
ReWriteRule ^state-to-state-transport([/]{0,1}+)$ pages/state-to-state-transport.php [NC,L]
#=============================================================================================================
#
# This route work for states-pages(states)
# auto transport state redirect
ReWriteRule ^auto-transport-states/([0-9A-Za-z\-]+)([/]?)$ https://rapidautoshipping.com/states/$1 [R=302,NC,L]

#==============================================================================================================

ReWriteRule ^states([/]?)$ pages/states/state_list.php [NC,L]








ReWriteRule ^states/kentucky([/]?)$ pages/states/kentucky.php [NC,L]


